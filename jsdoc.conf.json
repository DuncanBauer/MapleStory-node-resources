{
  "plugins": [
    "plugins/markdown"
    // "plugins/summarize"
  ],
  "source": {
    "include": [
      "./parser",
      "./browser"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "encoding"      : "utf8",
    "recurse"       : true,
    "private"       : false,
    "lenient"       : true,
    "destination"   : "./docs"
  }
}
